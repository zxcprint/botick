print("печать")
try:
    ot = float(input("от: "))
    do = float(input("до: "))
except ValueError:
    print("где-то опшипка")
    exit()
operation = input("тип печати: ")
if operation == "двусторонняя":
    result1 = (do - ot + 1) / 2
    print(f"количество листов = {result1}")
elif operation == "односторонняя":
    result2 = (do - ot + 1)   
    print(f"количество листов = {result2}")
else:
    print("неизвестный тип печати")
print("печать")
try:    
    ot = float(input("от"))
    do = float(input("до"))
except ValueError:
    print("где-то опшипка")
    exit()
operation = input("тип печати")
if operation == "двусторонняя":
    result = (do - ot + 1) / 2  
    print(f"количество листов = {result}")
elif operation == "односторонняя":
    result = (do - ot + 1)  
print(f"количество листов = {result}")
